<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:gravity="center_vertical" padding="2dip">
	<ImageView android:id="@+id/tree_item_class_icon" android:src="@drawable/icon"
		android:layout_height="wrap_content" android:layout_width="wrap_content" />
	<TextView android:text="0" android:visibility="gone"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:id="@+id/demo_list_item_level" android:textSize="22dp">
	</TextView>
	<TextView android:text="Text" android:layout_height="wrap_content"
		android:id="@+id/demo_list_item_description" android:textSize="22dp"
		android:layout_width="wrap_content">
	</TextView>
	<TextView android:text="Text" android:textColor="#008B8B"
		android:layout_height="wrap_content" android:id="@+id/demo_list_item_class"
		android:textSize="15dp" android:layout_width="wrap_content">
	</TextView>
	<CheckBox android:visibility="gone" android:text=""
		android:id="@+id/demo_list_checkbox" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:checked="false"
		android:padding="0dip" android:focusable="false"
		android:soundEffectsEnabled="true" android:focusableInTouchMode="true"></CheckBox>
	<ImageView android:id="@+id/tree_item_lock_icon"
		android:visibility="gone" android:src="@drawable/icon"
		android:layout_height="wrap_content" android:layout_width="wrap_content" />
</LinearLayout>
