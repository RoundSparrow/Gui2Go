<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="wrap_content">
	<LinearLayout android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@+id/linearLayout1"
		android:layout_marginBottom="4dip">
		<TextView android:id="@+id/textView1" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textSize="18dip"
			android:text="Current Value:"></TextView>
		<TextView android:id="@+id/rename_dialog_curr_name"
			android:layout_marginLeft="4dip" android:layout_width="wrap_content"
			android:textSize="18dip" android:layout_height="wrap_content"
			android:text=""></TextView>
	</LinearLayout>
	<LinearLayout android:layout_height="match_parent"
		android:layout_width="fill_parent" android:id="@+id/linearLayout2">
		<TextView android:id="@+id/textView3" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textSize="18dip"
			android:text="New Name:"></TextView>
		<EditText android:text="" android:layout_marginRight="4dip" android:maxLength="20"
			android:layout_width="fill_parent" android:id="@+id/rename_dialog_edittext"
			android:autoText="false" android:textSize="18dip" android:inputType="textNoSuggestions"
			android:layout_height="wrap_content" android:layout_marginLeft="4dip"></EditText>
	</LinearLayout>
</LinearLayout>
